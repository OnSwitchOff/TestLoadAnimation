            <PointAnimationUsingKeyFrames Storyboard.TargetName="GradientBrush" Storyboard.TargetProperty="StartPoint" AutoReverse="false" RepeatBehavior="Forever" AccelerationRatio="0" DecelerationRatio="0"  Duration="0:0:5">
                <LinearPointKeyFrame KeyTime="0%" Value="0.5,1"/>
                <LinearPointKeyFrame KeyTime="12.5%" Value="0.83,0.83"/>
                <LinearPointKeyFrame KeyTime="25%" Value="1,0.5"/>
                <LinearPointKeyFrame KeyTime="37.5%" Value="0.83,0.17"/>
                <LinearPointKeyFrame KeyTime="50%" Value="0.5,0"/>
                <LinearPointKeyFrame KeyTime="62.5%" Value="0.17,0.17"/>
                <LinearPointKeyFrame KeyTime="75%" Value="0,0.5"/>
                <LinearPointKeyFrame KeyTime="87.5%" Value="0.17,0.83"/>
                <LinearPointKeyFrame KeyTime="100%" Value="0.5,1"/>                
            </PointAnimationUsingKeyFrames>
            <PointAnimationUsingKeyFrames Storyboard.TargetName="GradientBrush" Storyboard.TargetProperty="EndPoint" AutoReverse="false" RepeatBehavior="Forever" AccelerationRatio="0" DecelerationRatio="0" Duration="0:0:5">
