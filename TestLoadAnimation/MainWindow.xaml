                                <LineSegment x:Name="line4" Point="35,90"/>
                                <LineSegment x:Name="line5" Point="30,90"/>
                            </PathFigure>
                        </PathGeometry>
                    </Path.Data>
                    <Path.Triggers>
                        <EventTrigger RoutedEvent="Path.Loaded">
                            <BeginStoryboard>
                                <Storyboard x:Name="sb">
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="MovingPart2" Storyboard.TargetProperty="StartPoint" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="30,60"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="30,50"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="30,50"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="arc1" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="70,60"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="70,50"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="30,50"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="30,50"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="arc2" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="35,60"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="35,50"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="35,50"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="35,50"/>
                                    </PointAnimationUsingKeyFrames>
                                    <SizeAnimationUsingKeyFrames Storyboard.TargetName="arc1" Storyboard.TargetProperty="Size" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearSizeKeyFrame KeyTime="0%" Value="20,20"/>
                                        <LinearSizeKeyFrame KeyTime="20%" Value="20,20"/>
                                        <LinearSizeKeyFrame KeyTime="60%" Value="0,20"/>
                                        <LinearSizeKeyFrame KeyTime="60%" Value="0,20"/>
                                        <LinearSizeKeyFrame KeyTime="100%" Value="0,20"/>
                                    </SizeAnimationUsingKeyFrames>
                                    <SizeAnimationUsingKeyFrames Storyboard.TargetName="arc2" Storyboard.TargetProperty="Size" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearSizeKeyFrame KeyTime="0%" Value="15,15"/>
                                        <LinearSizeKeyFrame KeyTime="20%" Value="15,15"/>
                                        <LinearSizeKeyFrame KeyTime="60%" Value="0,15"/>
                                        <LinearSizeKeyFrame KeyTime="60%" Value="0,15"/>
                                        <LinearSizeKeyFrame KeyTime="100%" Value="0,15"/>
                                    </SizeAnimationUsingKeyFrames>                                    
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="line1" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="70,70"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="70,60"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="30,60"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="30,60"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="line2" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="65,70"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="65,60"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="35,60"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="35,60"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="line3" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="65,60"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="65,50"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="35,50"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="35,50"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="line4" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="35,90"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="35,80"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="35,80"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="35,80"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="35,80"/>
                                    </PointAnimationUsingKeyFrames>
                                    <PointAnimationUsingKeyFrames Storyboard.TargetName="line5" Storyboard.TargetProperty="Point" Duration="0:0:1" AutoReverse="true" RepeatBehavior="Forever">
                                        <LinearPointKeyFrame KeyTime="0%" Value="30,90"/>
                                        <LinearPointKeyFrame KeyTime="20%" Value="30,80"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="30,80"/>
                                        <LinearPointKeyFrame KeyTime="60%" Value="30,80"/>
                                        <LinearPointKeyFrame KeyTime="100%" Value="30,80"/>
                                    </PointAnimationUsingKeyFrames>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Path.Triggers>
                </Path>
            </Border>

            <Border>
                <Border.Effect>
                    <DropShadowEffect Color="Gray" ShadowDepth="0" BlurRadius="5" Opacity="0.5" />
                </Border.Effect>
                <Path   StrokeThickness="1" Stroke="LightGray"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0">
                    <Path.Fill>
                        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                            <GradientStop Offset="0" Color="#bbcadd"/>
                            <GradientStop Offset="0.5" Color="#eef5fd"/>
                            <GradientStop Offset="1" Color="#bbcadd"/>
                        </LinearGradientBrush>
                    </Path.Fill>
                    <Path.Data>
                        <GeometryGroup>
                            <PathGeometry>
                                <PathFigure IsClosed="True" IsFilled="True" StartPoint="28,65">
                                    <LineSegment Point="72,65"/>
                                    <LineSegment Point="72,92"/>
                                    <LineSegment Point="28,92"/>
                                </PathFigure>
                            </PathGeometry>
                            <CombinedGeometry GeometryCombineMode="Union">
                                <CombinedGeometry.Geometry1>
                                    <EllipseGeometry Center="50,77" RadiusX="3" RadiusY="3"/>
                                </CombinedGeometry.Geometry1>
                                <CombinedGeometry.Geometry2>
                                    <PathGeometry>
                                        <PathFigure IsClosed="True" IsFilled="True" StartPoint="50,76">
                                            <LineSegment Point="52,85"/>
                                            <LineSegment Point="48,85"/>
                                        </PathFigure>
                                    </PathGeometry>
                                </CombinedGeometry.Geometry2>
                            </CombinedGeometry>
                        </GeometryGroup>
                    </Path.Data>
                </Path>
            </Border>

           
        </Canvas>

        <TextBlock x:Name="textBlock" Text="Loading..." Grid.ColumnSpan="2" Grid.RowSpan="1" VerticalAlignment="Center" FontSize="50" HorizontalAlignment="Center">
            <TextBlock.Resources>
                <Storyboard x:Key="testStoryboard">                    
                    <DoubleAnimation From="50" To="80" Storyboard.TargetProperty="FontSize" AutoReverse="True" RepeatBehavior="Forever" Duration="0:0:0.5"/>                    
                </Storyboard>                
            </TextBlock.Resources>
            <TextBlock.Style>
                <Style>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding IsLoading}" Value="True">
                            <DataTrigger.EnterActions>
                                <BeginStoryboard x:Name="myStoryBoard" Storyboard="{StaticResource testStoryboard}"/>
                            </DataTrigger.EnterActions>
                            <DataTrigger.ExitActions>
                                <PauseStoryboard BeginStoryboardName="myStoryBoard"/>
                            </DataTrigger.ExitActions>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </TextBlock.Style>
        </TextBlock>
        
        <Button Grid.Column="0" Grid.Row="1" Content="Start" Margin="10" Click="StartBtn_Click" x:Name="startBtn" ></Button>
        <Button Grid.Column="1" Grid.Row="1" Content="Stop" Margin="10" Click="StopBtn_Click" Template="{StaticResource btnTemplate}" Width="50" Height="50"></Button>
    </Grid>
</Window>
